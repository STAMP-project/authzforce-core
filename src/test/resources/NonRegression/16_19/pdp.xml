<?xml version="1.0" encoding="UTF-8"?>
<pdps xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://thalesgroup.com/authzforce/pdp/model/2014/12"
	xmlns:test="http://thalesgroup.com/authzforce/model/3.0/finder/attribute/test"
	defaultAttributeFactory="attr" defaultCombiningAlgFactory="comb" defaultFunctionFactory="func"
	defaultPDP="TestPDP">
	<pdp name="TestPDP">
		<policyFinder xsi:type="StaticPolicyFinder">
			<policyLocation>policy.xml</policyLocation>
		</policyFinder>
		<!-- <attributeFinder xsi:type="CurrentDateTimeFinder" /> -->
		<!-- <attributeFinder xsi:type="AttributeSelectorXPathFinder" /> -->
		<attributeFinder xsi:type="test:TestAttributeFinder">
			<test:providedAttribute Category="urn:oasis:names:tc:xacml:1.0:subject-category:access-subject"
				AttributeId="urn:oasis:names:tc:xacml:2.0:subject:role" DataType="http://www.w3.org/2001/XMLSchema#string"
				MustBePresent="true" />
		</attributeFinder>
	</pdp>
	<attributeFactory name="attr" useStandardDatatypes="true" />
	<combiningAlgFactory name="comb" useStandardAlgorithms="true" />
	<functionFactory name="func" useStandardFunctions="true" />
</pdps>